		/* Template used to remove speech bubble PNG and replace with CSS Triangle. */
		{ "selector": "#comments .even", "type": "background-color", "label": "Area", "template": "$selector { background-color: $value; } $selector:before { content: ''; position:absolute; width: 0px; height: 0px; border-left: 28px solid transparent; border-right: 0px solid transparent; border-top: 27px solid $value; }" },

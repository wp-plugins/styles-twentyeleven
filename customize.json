		{ "selector": "img[class*='align'], img[class*='wp-image - '], #content .gallery .gallery-icon img, .wp-caption img", "type": "border-color", "label": "Image" },  { "selector": ".entry-content .wp-caption-text", "type": "text", "label": "Caption" },

	"Entry – Meta": [
		/* Meta */
		{ "selector": ".entry-meta", "type": "text", "label": "Meta" },
		{ "selector": ".entry-meta", "type": "color", "label": "Meta" },
		{ "selector": ".entry-meta a", "type": "color", "label": "Meta link" },
		{ "selector": ".entry-meta a:hover", "type": "color", "label": "Meta link" },
